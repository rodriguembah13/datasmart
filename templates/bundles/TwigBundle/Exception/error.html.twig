{% extends 'base.html.twig' %}
{% block page_title %} Error Page{% endblock %}
{% block page_subtitle %} {{ status_code }} {% endblock %}
{% block navbar_start %}

{% endblock %}
{% block page_content %}
    <div class="error-page">
        <h2 class="headline col-md-6" style="color: #801419">{{ status_code }}</h2>

        <div class="error-content">
            <h3>{{ 'Oops!'|trans() }}</h3>

            <p>{{ exception.message }}</p>
        </div>
    </div>

{% endblock %}